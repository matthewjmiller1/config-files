Host *.mattandleighann.com
    # Files to use for authentication
    IdentityFile ~/.ssh/id_ed25519_dreamhost

Host github.com
    # Files to use for authentication
    IdentityFile ~/.ssh/id_ed25519_github

# Default settings, applied to all hosts.
# Any more-specific host settings should go above this and will override an
# settings in the default settings.
Host *
    AddKeysToAgent yes
    UseKeychain yes
